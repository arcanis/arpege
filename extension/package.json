{
  "name": "supersyntax",
  "publisher": "arcanis",
  "version": "0.0.6",
  "displayName": "SuperSyntax - custom grammars for VSCode",
  "description": "Adds support for PEG.js-powered languages in VSCode",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/arcanis/pegjs.git",
    "directory": "extension"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./lib/extension.js",
  "activationEvents": [
    "onDebug",
    "onLanguage:supersyntax"
  ],
  "contributes": {
    "configuration": {
      "title": "SuperSyntax",
      "properties": {
        "supersyntax.parsers": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "description": "Parsers to enable."
        }
      }
    },
    "grammars": [
      {
        "language": "supersyntax",
        "scopeName": "source.supersyntax",
        "path": "./supersyntax.tmGrammar.json",
        "embeddedLanguages": {
          "meta.embedded.javascript": "javascript"
        }
      }
    ],
    "languages": [
      {
        "id": "supersyntax",
        "extensions": [
          ".stx"
        ],
        "aliases": [
          "SuperSyntax"
        ],
        "filenames": [],
        "configuration": "./language-configuration.json"
      }
    ],
    "semanticTokenScopes": [
      {
        "scopes": {
          "code:js": [
            "meta.embedded.javascript"
          ]
        }
      }
    ]
  },
  "dependencies": {
    "pegjs": "workspace:*",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/vscode": "^1.67.0",
    "rollup": "^2.72.1",
    "rollup-plugin-pegjs": "^2.1.3",
    "typescript": "^4.6.4",
    "vsce": "^2.7.0"
  },
  "scripts": {
    "build:extension": "yarn prepack",
    "prepack": "yarn rollup -c",
    "postpack": "rm -rf lib"
  }
}
